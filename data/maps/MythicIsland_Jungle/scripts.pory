mapscripts MythicIsland_Jungle_MapScripts {
    MAP_SCRIPT_ON_TRANSITION {
        if(flag(FLAG_MYTHIC_ISLAND_MEW_FIRST_SIGHTING)) {
            setvar(VAR_TEMP_0, 1)
        }
    }
    MAP_SCRIPT_ON_LOAD {
        if(!flag(FLAG_MYTHIC_ISLAND_TEMPLE_OPEN)) {
            setmetatile(28, 43, METATILE_General_RockWall_GrassBase, TRUE)
            setmetatile(28, 42, METATILE_General_RockWall_RockBase, TRUE)
        }
    }
}

movement MewRunAway {
    walk_up * 9
    walk_left
    face_down 
}

movement ChaseMewLeft {
    walk_up * 9
    walk_left
    face_up
}

script MythicIsland_Jungle_ApproachMewLeft {
    lockall
    playse(SE_PIN)
    applymovement(MEW_OBJECT, Common_Movement_ExclamationMark)
    waitmovement(0)
    delay(30)
    applymovement(MEW_OBJECT, Common_Movement_FaceDown)
    waitmovement(0)
    delay(30)
    playmoncry(SPECIES_MEW, CRY_MODE_NORMAL)
    applymovement(MEW_OBJECT, MewRunAway)
    delay(30)
    applymovement(OBJ_EVENT_ID_PLAYER, ChaseMewLeft)
    waitmovement(0)
    playse(SE_M_PSYBEAM)
    applymovement(MEW_OBJECT, teleportSpin)
    waitmovement(0)
    playse(SE_M_TELEPORT)
    applymovement(MEW_OBJECT, teleportFly)
    waitmovement(0)
    setflag(FLAG_MYTHIC_ISLAND_MEW_FIRST_SIGHTING)
    removeobject(MEW_OBJECT)
    setvar(VAR_TEMP_0, 1)
    release
}

