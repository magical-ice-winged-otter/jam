script MythicIsland_Jungle_AllKeys {
    if(flag(FLAG_MYTHIC_ISLAND_WATER_PLATE_INSERTED) && flag(FLAG_MYTHIC_ISLAND_FIRE_PLATE_INSERTED) && flag(FLAG_MYTHIC_ISLAND_THUNDER_PLATE_INSERTED)) {
        setflag(FLAG_MYTHIC_ISLAND_TEMPLE_OPEN)
        lockall
        setvar(VAR_0x8004, 1)
        setvar(VAR_0x8005, 1)
        setvar(VAR_0x8006, 8)
        setvar(VAR_0x8007, 3)
        special(ShakeCamera)
        waitstate
        delay(30)
        setvar(VAR_0x8004, 1)
        setvar(VAR_0x8005, 1)
        setvar(VAR_0x8006, 16)
        setvar(VAR_0x8007, 3)
        special(ShakeCamera)
        waitstate
        delay(30)
        playse(SE_EFFECTIVE)
        setmetatile(28, 43, METATILE_General_CaveEntrance_Bottom, FALSE)
        setmetatile(28, 42, METATILE_General_CaveEntrance_Top, TRUE)
        special(DrawWholeMapView)
        delay(30)
        waitse
        releaseall
        end
    }
    closemessage
    release
}



// Pokemon
script MythicIsland_Jungle_Encounter_Yanma {
    lock 
    faceplayer
    setflag(B_SMART_WILD_AI_FLAG)
    setflag(B_FLAG_NO_CATCHING)
    setwildbattle(SPECIES_NONE, 0)
    createmon(B_SIDE_OPPONENT, B_FLANK_LEFT, SPECIES_YANMA, 30, ITEM_WIDE_LENS, NUM_NATURES, 1, MON_GENDERLESS, 0, 0, 0, 0, 0, 0, 0, 31, 31, 31, 31, 31, 31, MOVE_SIGNAL_BEAM, MOVE_WING_ATTACK, MOVE_DOUBLE_TEAM, MOVE_DETECT)
    playmoncry(SPECIES_YANMA, CRY_MODE_ENCOUNTER)
    waitmoncry
    setflag(FLAG_SYS_CTRL_OBJ_DELETE)
    dowildbattle
    clearflag(B_SMART_WILD_AI_FLAG)
    clearflag(B_FLAG_NO_CATCHING)
    specialvar(VAR_RESULT, GetBattleOutcome)
        
    switch(var(VAR_RESULT)) {
        case B_OUTCOME_LOST:
            break
        case B_OUTCOME_RAN:
            break
        case B_OUTCOME_WON:
            setflag(YANMA_FLAG)
            removeobject(YANMA_OBJECT)
            goto(Common_EventScript_RemoveStaticPokemon)
        case B_OUTCOME_CAUGHT:
            setflag(YANMA_FLAG)
            removeobject(YANMA_OBJECT)
            goto(Common_EventScript_RemoveStaticPokemon)
        default:
            setflag(YANMA_FLAG)
            removeobject(YANMA_OBJECT)
            goto(Common_EventScript_RemoveStaticPokemon)
    }
    release
}

