script MythicIsland_Jungle_KeyHole1 {
    lock
    if(flag(FLAG_MYTHIC_ISLAND_THUNDER_PLATE_INSERTED)) {
        msgbox(format(
            "The Zap Plate fits perfectly."
        ))
    }
    else {
        msgbox(format(
            "It looks like a stone plate of sorts could be placed in here."
        ))
        checkitem(ITEM_ZAP_PLATE) 
        if (var(VAR_RESULT)) {
            msgbox(format(
                "Insert the Zap Plate inside?"
            ), MSGBOX_YESNO)
            if(var(VAR_RESULT) == YES) {
                playse(SE_M_DETECT)
                waitse
                delay(30)
                removeitem(ITEM_ZAP_PLATE)
                msgbox(format(
                    "The Zap Plate fits perfectly."
                ))
                closemessage
                setflag(FLAG_MYTHIC_ISLAND_THUNDER_PLATE_INSERTED)
                call(MythicIsland_Jungle_AllKeys)
            }
        } 
    }
    closemessage
    release
}

